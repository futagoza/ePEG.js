{
  "name": "epeg.js",
  "version": "0.0.0-checkpoint",
  "description": "ePEG.js - Extended PEG.js for Node.js 4+",
  "keywords": [
    "pegjs",
    "epeg",
    "peg",
    "parser",
    "generator",
    "extended",
    "plugins",
    "include",
    "import",
    "template",
    "ecmascript",
    "es2015",
    "es2016",
    "coffeescript",
    "ast"
  ],
  "homepage": "https://github.com/futagoza/ePEG.js",
  "repository": "futagoza/ePEG.js",
  "bugs": "https://github.com/futagoza/ePEG.js/issues",
  "license": "MIT",
  "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",

  "main": "lib/epeg.js",
  "bin": {
    "epeg": "bin/epeg.js",
    "pegjs": "bin/peg.js"
  },
  "scripts": {
    "rebuild": "npm run clean && npm run build",
    "generate-parser": "mkdirp lib && pegjs --output lib/parser.js src/parser.pegjs",
    "build": "babel src --out-dir . && npm run generate-parser",
    "build-bin": "babel src/bin --out-dir bin",
    "build-lib": "babel src/lib --out-dir lib",
    "build-benchmark": "babel src/benchmark --out-dir benchmark",
    "build-spec": "babel src/spec --out-dir spec",
    "clean": "rimraf bin benchmark lib spec",
    "benchmark": "node benchmark/run",
    "spec": "mocha spec --recursive",
    "test": "npm run benchmark && npm run spec"
  },
  "dependencies": {
    "fs-extra": "0.30.0",
    "pegjs-dev": "0.9.119",
    "babel-runtime": "6.9.1"
  },
  "devDependencies": {
    "mkdirp": "0.5.1",
    "babel-cli": "6.10.1",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "rimraf": "2.5.2",
    "mocha": "2.5.3"
  },
  "engines": {
    "node": ">= 4.0.0"
  }
}
