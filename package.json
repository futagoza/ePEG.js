{
  "name": "epeg.js",
  "version": "0.0.0-checkpoint",
  "description": "ePEG.js - Extended PEG.js for Node.js 4+",
  "keywords": [
    "pegjs",
    "epeg",
    "peg",
    "parser",
    "generator",
    "extended",
    "plugins",
    "include",
    "import",
    "template",
    "ecmascript",
    "es2015",
    "es2016",
    "coffeescript",
    "ast"
  ],
  "homepage": "https://github.com/futagoza/ePEG.js",
  "repository": "futagoza/ePEG.js",
  "bugs": "https://github.com/futagoza/ePEG.js/issues",
  "license": "MIT",
  "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",

  "main": "lib/epeg.js",
  "bin": {
    "epeg": "bin/epeg.js",
    "pegjs": "bin/peg.js"
  },
  "scripts": {
    "rebuild": "npm run clean && npm run build",
    "build": "npm run generator && npm run transpiler",
    "generator": "node scripts/generate-parser.js",
    "transpiler": "node scripts/transpile.js",
    "transpile:bin": "node scripts/transpile.js -R bin",
    "transpile:lib": "node scripts/transpile.js -R lib",
    "transpile:benchmark": "node scripts/transpile.js -R benchmark",
    "transpile:spec": "node scripts/transpile.js -R spec",
    "clean": "node scripts/remove.js",
    "benchmark": "node benchmark/run",
    "spec": "mocha spec --recursive",
    "test": "npm run benchmark && npm run spec"
  },
  "dependencies": {
    "commander": "2.9.0",
    "globby": "5.0.0",
    "mkdirp": "0.5.1",
    "pegjs-dev": "0.9.128",
    "babel-runtime": "6.9.2"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "rimraf": "2.5.3",
    "mocha": "2.5.3"
  },
  "engines": {
    "node": ">= 4.0.0"
  }
}
